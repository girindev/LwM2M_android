<?xml version="1.0" encoding="utf-8"?>
<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
	<Object ObjectType="MODefinition">
		<Name>Battery Status</Name>
		<Description1>Description: This IPSO object should be used as battery status monitor.</Description1>
		<ObjectID>9002</ObjectID>
		<ObjectURN>urn:oma:lwm2m:ext:9002</ObjectURN>
		<MultipleInstances>Multiple</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="5601">
				<Name>Min Measured Value</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>Defined by “Units” resource.</Units>
				<Description>The low threshold of battery level.</Description>
			</Item>
			<Item ID="5524">
				<Name>Duration</Name>
				<Operations>RW</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>Float</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units>Defined by “Units” resource.</Units>
				<Description>When the power reaches the threshold, it will alarm once for every 5% decrease</Description>
			</Item>
			<Item ID="5527">
				<Name>Text</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Optional</Mandatory>
				<Type>String</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description>for client to report battery level</Description>
			</Item>
			<Item ID="5">
				<Name>Timestamp</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Time</Type>
				<RangeEnumeration/>
				<Units/>
				<Description>
					The timestamp of when the location measurement was performed.
				</Description>
			</Item>
		</Resources>
		<Description2></Description2>
	</Object>
</LWM2M>
